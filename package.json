{
  "name": "react-template",
  "version": "1.5.0",
  "description": "react-template",
  "main": "index.js",
  "repository": "https://github.com/TobiahRex/reactBoilerplate.git",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm-run-all start-env",
    "pre-start": "babel-node env-setup.js",
    "start-env": "if test \"$NODE_ENV\" = \"production\" ; then npm run start-prod; else npm run start-dev; fi ",
    "start-dev": "npm-run-all dev-env-setup open:dev",
    "start-prod": "npm-run-all prod-env-setup open:dist",
    "open:dev": "babel-watch ./server/server.js",
    "open:dist": "babel-node ./tools/distServer.js",
    "dev-env-setup": "babel-node ./tools/dev-env-setup.js",
    "prod-env-setup": "babel-node ./tools/prod-env-setup.js",
    "remove-dist": "rimraf ./dist && mkdir dist",
    "copy-images": "copyfiles -u 1 './src/Images/**' ./dist/",
    "copy-package": "copyfiles 'package.json' ./dist/",
    "prebuild": "npm run remove-dist",
    "build:html": "babel-node ./tools/buildHtml.js",
    "build:bundle": "babel-node ./tools/build.js",
    "build": "NODE_ENV=production npm-run-all build:html build:bundle copy-images open:dist"
  },
  "author": "Tobiah Rex",
  "license": "ISC",
